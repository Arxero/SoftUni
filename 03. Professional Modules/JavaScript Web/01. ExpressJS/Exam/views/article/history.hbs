     <h1>Welcome to SoftUni Wiki!</h1>
                <section>
                    <h2>{{title}}</h2>
                    <div class="spacer">
                        <h3>Edit history</h3>
                        <ul>
                            {{#each edits}}
                                <li>
                                    <a href="/view/edit/{{this._id}}">{{this.date}}</a> by
                                    <span class="author">{{this.author}}</span>
                                </li>
                            {{/each}}
                        </ul>
                    </div>
                </section>